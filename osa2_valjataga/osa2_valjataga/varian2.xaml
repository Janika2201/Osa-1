<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="osa2_valjataga.varian2">
    <ContentPage.Content>
        <StackLayout>
            <DatePicker x:Name="changehoros" DateSelected="changehoros_DateSelected" MinimumDate="01/01/2020"
                        MaximumDate="12/31/2020"
                        Date="7/7/2020"/>
            <Image x:Name="horos" Source="znak.jpg"/>
            <Entry x:Name="info" Text="Kes sa oled?"/>
            <Label Text=""
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>